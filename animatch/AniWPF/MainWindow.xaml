<Window x:Class="AniWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AniWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBlock x:Name="data"></TextBlock>
            <Button x:Name="getAnime" Padding="10" Click="getAnime_Click">Get Data</Button>
            <Button x:Name="openChildForm" Padding="10" Click="openChildForm_Click">Open Child Form</Button>        </StackPanel>
    </Grid>
</Window>
