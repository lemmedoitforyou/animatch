<Window x:Class="AniWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AniWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="1024" Width="1440">
    <Grid Background="LightPink">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBlock x:Name="data"></TextBlock>
        </StackPanel>
        <TextBox x:Name="login" TextWrapping="Wrap" Text="" Margin="494,457,465,496" TextChanged="login"/>
    </Grid>
</Window>
